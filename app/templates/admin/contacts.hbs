<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>E-mail</th>
      <th>Message</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |contact|}}
      <tr>
        <th>{{contact.email}}</th>
        <td>{{contact.message}}</td>
        <td><button type="button" class="btn btn-xs" {{action 'deleteContact' contact}}>Delete</button></td>
      </tr>
    {{/each}}
  </tbody>
</table>
